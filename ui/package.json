		"rollup-plugin-babel": "^4.4.0",
		"rollup-plugin-cleanup": "^3.1.1",
		"rollup-plugin-svelte": "^7.0.0",
		"rollup-plugin-terser": "^7.0.2",
		"sapper": "^0.29.1",
