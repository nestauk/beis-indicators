	},
	"license": "MIT",
	"name": "beis_indicators_ui",
	"scripts": {
		"build": "sapper build --legacy",
		"chmod": "chmod u+x src/bin/*",
		"copydata": "npm run chmod && node -r esm src/bin/copy_data.js",
		"dev": "sapper dev",
		"export": "export SAPPER_EXPORT=true && sapper export --legacy",
		"linksvizzles": "npm link @svizzle/barchart @svizzle/choropleth @svizzle/legend @svizzle/time_region_value @svizzle/ui @svizzle/utils @svizzle/dev @svizzle/file",
		"makedata": "npm run makeindex && npm run rmdata && npm run copydata",
		"makeindex": "npm run chmod && node -r esm src/bin/make_index.js",
		"rmdata": "rimraf static/data",
		"rmsappers": "rimraf __sapper__ src/node_modules/@sapper",
		"start": "node __sapper__/build",
		"test": "run-p --race dev cy:run",
		"unittest": "mocha -r esm --spec src/**/*.spec.js"
	}
}
